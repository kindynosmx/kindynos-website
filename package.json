{
  "name": "@kindynosmx/website",
  "version": "1.0.0",
  "private": false,
  "description": "We build it!",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore '**/*.{md,json,js,jsx,ts,tsx,css}' --write",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore --ignore-path .eslintignore --cache --cache-location .husky/.eslintcache/ \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "start": "next start",
    "tscheck": "tsc -p tsconfig.json --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.3",
    "@chakra-ui/react": "^2.6.1",
    "@ducanh2912/next-pwa": "^8.4.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@sentry/nextjs": "^7.51.2",
    "@vercel/analytics": "^1.0.1",
    "framer-motion": "^10.12.10",
    "lodash": "^4.17.21",
    "next": "^13.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.2",
    "@next/eslint-plugin-next": "^13.4.2",
    "@types/node": "^20.1.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-chakra-ui": "^0.8.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-custom-order-fix": "^0.1.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
